	<cache alias="denominacioOrganisme" uses-template="default"><key-type>java.lang.String</key-type><value-type>java.lang.String</value-type><expiry><ttl unit="seconds">604800</ttl></expiry><resources><heap unit="entries">300</heap></resources></cache>
