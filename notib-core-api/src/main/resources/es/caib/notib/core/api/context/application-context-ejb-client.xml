<?xml version="1.0" encoding="UTF-8"?>
<beans	xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:jee="http://www.springframework.org/schema/jee"
		xsi:schemaLocation="
				http://www.springframework.org/schema/beans
				http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
				http://www.springframework.org/schema/jee
				http://www.springframework.org/schema/jee/spring-jee-3.0.xsd">

	<jee:local-slsb
			id="aplicacioService"
			jndi-name="notib/AplicacioServiceBean/local"
			business-interface="es.caib.notib.core.api.service.AplicacioService"/>
	<jee:local-slsb
			id="entitatService"
			jndi-name="notib/EntitatServiceBean/local"
			business-interface="es.caib.notib.core.api.service.EntitatService"/>
	<jee:local-slsb
			id="notificacioService"
			jndi-name="notib/NotificacioServiceBean/local"
			business-interface="es.caib.notib.core.api.service.NotificacioService"/>
	<jee:local-slsb
			id="usuariAplicacioService"
			jndi-name="notib/UsuariAplicacioServiceBean/local"
			business-interface="es.caib.notib.core.api.service.UsuariAplicacioService"/>

	<jee:local-slsb
			id="notificacioWsService"
			jndi-name="notib/NotificacioWsServiceBean/local"
			business-interface="es.caib.notib.core.api.ws.notificacio.NotificacioWsService"/>
	<jee:local-slsb
			id="adviserWsService"
			jndi-name="notib/NotificaAdviserWsBean/local"
			business-interface="es.caib.notib.core.wsdl.adviser.AdviserWS"/>

</beans>
